class Solution {
    public int secondLargestElement(int[] nums) {
    int res=nums[0];
    int sec=-1;
    for(int i=1;i<nums.length;i++){
        res=Math.max(nums[i],res);
        if(nums[i]>res&&nums[i]!=res){
            sec=res;
            res=nums[i];
        }
        else{
            if(nums[i]>sec&&nums[i]!=res){
                sec=nums[i];
            }
        }
    }
    return sec;
    }
}
